{
  "name": "remix-template-cloudflare-workers",
  "private": true,
  "sideEffects": false,
  "main": "build/index.js",
  "version": "o.1.0",
  "scripts": {
    "deploy": "npm run build && wrangler publish",
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch --wrangler-config wrangler.dev.toml",
    "dev2": "remix build && run-p dev:*",
    "dev": "wrangler dev",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "logs": "wrangler tail remix-mozart409"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.18",
    "@next-boost/hybrid-disk-cache": "^0.3.0",
    "@remix-run/cloudflare": "^1.16.1",
    "@remix-run/cloudflare-workers": "^1.16.1",
    "@remix-run/react": "^1.16.1",
    "cross-env": "^7.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remix-image": "^1.4.0",
    "remix-image-wasm": "^0.1.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230511.0",
    "@remix-run/dev": "^1.16.1",
    "@remix-run/eslint-config": "^1.16.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "eslint": "^8.40.0",
    "miniflare": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
