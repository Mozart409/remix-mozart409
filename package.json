{
  "name": "remix-template-cloudflare-workers",
  "private": true,
  "sideEffects": false,
  "main": "build/index.js",
  "version": "o.1.0",
  "scripts": {
    "deploy": "npm run build && wrangler publish",
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch --wrangler-config wrangler.dev.toml",
    "dev2": "remix build && run-p dev:*",
    "dev": "wrangler dev",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "logs": "wrangler tail remix-mozart409"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@next-boost/hybrid-disk-cache": "^0.3.0",
    "@remix-run/cloudflare": "^2.0.1",
    "@remix-run/cloudflare-workers": "^2.0.1",
    "@remix-run/react": "^2.0.1",
    "cross-env": "^7.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remix-image": "^1.4.0",
    "remix-image-wasm": "^0.1.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230922.0",
    "@remix-run/dev": "^2.0.1",
    "@remix-run/eslint-config": "^2.0.1",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.1",
    "eslint": "^8.50.0",
    "miniflare": "^3.20230922.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.30",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18"
  }
}
