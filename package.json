{
  "name": "remix-template-cloudflare-workers",
  "private": true,
  "sideEffects": false,
  "main": "build/index.js",
  "scripts": {
    "deploy": "npm run build && wrangler publish",
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch --wrangler-config wrangler.dev.toml",
    "dev": "remix build && run-p dev:*",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "logs": "wrangler tail remix-mozart409"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.3",
    "@heroicons/react": "^1.0.6",
    "@remix-run/cloudflare": "^1.5.1",
    "@remix-run/cloudflare-workers": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "cross-env": "^7.0.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.10.0",
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@tailwindcss/forms": "^0.5.2",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "eslint": "^8.15.0",
    "miniflare": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
