{
  "name": "remix-template-cloudflare-workers",
  "private": true,
  "sideEffects": false,
  "main": "build/index.js",
  "scripts": {
    "deploy": "npm run build && wrangler publish",
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch --wrangler-config wrangler.dev.toml",
    "dev": "remix build && run-p dev:*",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "logs": "wrangler tail remix-mozart409"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^2.0.10",
    "@remix-run/cloudflare": "^1.7.0",
    "@remix-run/cloudflare-workers": "^1.7.0",
    "@remix-run/react": "^1.7.0",
    "cross-env": "^7.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.16.0",
    "@remix-run/dev": "^1.7.0",
    "@remix-run/eslint-config": "^1.7.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.6",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "eslint": "^8.23.0",
    "miniflare": "^2.7.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": ">=14"
  }
}
